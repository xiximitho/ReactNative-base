{
  "extends": ["airbnb", "prettier", "eslint-config-prettier"],
  "parser": "babel-eslint",
  "rules": {
    "import/no-unresolved": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error",
      { "singleQuote": true, "printWidth": 100, "parser": "flow" }
    ]
  },
  "plugins": ["prettier"]
}
